  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/hapi": "^18.0.8",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.13.0",
    "chai": "^4.3.7",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "mocha": "^10.2.0",
